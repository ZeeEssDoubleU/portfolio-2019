{
	"name": "portfolio",
	"private": true,
	"description": "Zachary Williams' portfolio.",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"prebuild": "node -r esm prebuild",
		"build": "yarn prebuild && next build",
		"clean": "rm -rf .next && yarn dev",
		"clean-build": "rm -rf .next && yarn build && yarn start",
		"deep-clean": "rm -rf .next && rm -rf node_modules && yarn && yarn dev",
		"dev": "yarn prebuild && next dev",
		"dev-m": "yarn prebuild && next dev -o -H $HOSTNAME -p 8000",
		"dev-codegen": "yarn get-schema && yarn dev",
		"format": "prettier --write src/**/*.{js, jsx, ts, tsx}",
		"start": "next start",
		"serve": "yarn start -o",
		"serve-m": "yarn start -o -H $HOSTNAME -p 9000",
		"type-check": "tsc --watch",
		"generate": "graphql-codegen --config codegen.yml",
		"get-schema": "node -r esm src/api/shopify/graphql/schema/getSchema.js",
		"get-assets": "node -r esm src/utils/downloadAssets.js"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"body-scroll-lock": "^3.1.5",
		"framer-motion": "^4.1.16",
		"graphql-request": "^3.4.0",
		"gsap": "^3.6.1",
		"intersection-observer": "^0.12.0",
		"next": "^10.2.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"react-icons": "^4.2.0",
		"react-query": "^3.16.0",
		"styled-components": "^5.3.0"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.13.0",
		"@graphql-codegen/cli": "^1.21.4",
		"@graphql-codegen/introspection": "^1.18.2",
		"@graphql-codegen/typescript": "^1.22.0",
		"@graphql-codegen/typescript-graphql-request": "^3.2.0",
		"@graphql-codegen/typescript-operations": "^1.17.16",
		"@graphql-codegen/typescript-react-query": "^1.3.0",
		"@types/node": "^15.3.0",
		"@types/react": "^17.0.5",
		"@types/react-dom": "^17.0.5",
		"@types/react-helmet": "^6.1.1",
		"@types/styled-components": "^5.1.9",
		"@types/webpack": "^5.28.0",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"babel-plugin-styled-components": "^1.12.0",
		"concurrently": "^6.1.0",
		"dotenv": "^9.0.2",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-react": "^7.23.2",
		"esm": "^3.2.25",
		"type-coverage": "^2.17.5",
		"typescript": "^4.2.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ZeeEssDoubleU/portfolio-2019"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	}
}
