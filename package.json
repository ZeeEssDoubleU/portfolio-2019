{
	"name": "portfolio",
	"private": true,
	"description": "Zachary Williams' portfolio.",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
    "prebuild": "node -r esm gatsby_config/prebuild",
		"build": "yarn prebuild && gatsby build",
		"clean": "gatsby clean && yarn dev",
    "clean-build": "gatsby clean && gatsby build && gatsby serve",
    "deep-clean": "gatsby clean && rm -rf node_modules && yarn && yarn dev",
		"dev": "yarn prebuild && gatsby develop",
		"dev-m": "yarn prebuild && gatsby develop -o -H $HOSTNAME -p 8000",
		"format": "prettier --write src/**/*.{js,jsx}",
		"start": "npm run develop",
		"serve": "gatsby serve -o",
		"serve-m": "gatsby serve -o -H $HOSTNAME -p 9000",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
	},
	"dependencies": {
		"@reach/router": "^1.3.4",
		"axios": "^0.21.1",
		"body-scroll-lock": "^3.1.5",
		"framer-motion": "^4.1.16",
		"gatsby": "^3.5.0",
		"gatsby-image": "^3.5.0",
		"gatsby-plugin-google-analytics": "^3.5.0",
		"gatsby-plugin-manifest": "^3.5.0",
		"gatsby-plugin-modal-routing": "^1.2.0",
		"gatsby-plugin-offline": "^4.5.0",
		"gatsby-plugin-react-helmet": "^4.5.0",
		"gatsby-plugin-robots-txt": "^1.6.2",
		"gatsby-plugin-sharp": "^3.5.0",
		"gatsby-plugin-sitemap": "^4.1.0",
		"gatsby-plugin-styled-components": "^4.5.0",
		"gatsby-source-datocms": "^2.6.15",
		"gatsby-transformer-json": "^3.5.0",
		"gatsby-transformer-sharp": "^3.5.0",
		"gsap": "^3.6.1",
		"intersection-observer": "^0.12.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"react-icons": "^4.2.0",
		"styled-components": "^5.3.0"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.13.0",
		"@types/node": "^15.0.3",
		"@types/react": "^17.0.5",
		"@types/react-dom": "^17.0.5",
		"@types/react-helmet": "^6.1.1",
		"@types/styled-components": "^5.1.9",
		"@types/webpack": "^5.28.0",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"babel-plugin-styled-components": "^1.12.0",
		"dotenv": "^9.0.2",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-react": "^7.23.2",
		"esm": "^3.2.25",
		"gatsby-plugin-ts-config": "^1.1.5",
		"type-coverage": "^2.17.5",
		"typescript": "^4.2.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ZeeEssDoubleU/portfolio-2019"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	}
}
